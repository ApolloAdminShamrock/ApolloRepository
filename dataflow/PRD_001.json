				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> Cloud360",
				"StagingIn, MaxKey join(1==1,",
				"     joinType:'cross',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> join1",
				"join1, Cloud360 lookup(ItemKey == ItemKey,",
				"     multiple: false,",
				"     pickup: 'any',",
				"     broadcast: 'auto')~> lookup1",
				"lookup1 split(isNull(ItemKey),",
				"     disjoint: false) ~> split1@(New, Exists)",
				"split1@New derive(RecordStatusInsert = 1,",
				"          Cloud360_RecordEffectiveStartDate = currentTimestamp(),",
				"          Cloud360_RecordEffectiveEndDate = '2099-12-31 00:00:01.000') ~> derivedColumn1",
				"derivedColumn1 derive(PRD_SID = toInteger(byName('PRD_SID')),",
				"          MaxKey = toInteger(byName('MaxKey'))) ~> MapDrifted1",
				"split1@Exists derive(RecordStatus = 0) ~> derivedColumn2",
				"split1@Exists derive(RecordStatus = 1) ~> derivedColumn3",
				"derivedColumn2 filter(RecordStatus==1) ~> filter1",
				"filter1 alterRow(updateIf(RecordStatus==1)) ~> alterRow1",
				"Cloud360 select(skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"MapDrifted1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
