{
	"name": "Apollo Inventory Pipeline",
	"properties": {
		"activities": [
			{
				"name": "Inventory csv COPY",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false
				},
				"outputs": [
					{
						"referenceName": "InventoryStagingTable",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}